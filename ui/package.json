{
  "scripts": {
    "test": "karma start karma-config.js",
    "start": "webpack-dev-server --content-base app --port 9090 --inline --hot",
    "build": "rm -rf dist && NODE_ENV=production webpack && cp app/index.html dist/webapp/index.html && cp -R app/assets dist/webapp",
